{
    "name": "pfe",
    "version": "1.0.0",
    "description": "Backend API for PFE Project",
    "main": "dist/server.js",
    "scripts": {
        "dev": "tsx watch src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "lint": "eslint . --ext .ts",
        "format": "prettier --write \"src/**/*.ts\"",
        "test": "jest --passWithNoTests",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "prepare": "husky install",
        "type-check": "tsc --noEmit"
    },
    "keywords": [
        "node",
        "typescript",
        "express",
        "mongodb"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^6.0.0",
        "bcryptjs": "^3.0.3",
        "cors": "^2.8.5",
        "dotenv": "^17.2.3",
        "express": "^4.18.2",
        "express-async-errors": "^3.1.1",
        "helmet": "^8.0.0",
        "http-status-codes": "^2.3.0",
        "jsonwebtoken": "^9.0.2",
        "mongodb": "^5.9.2",
        "morgan": "^1.10.0",
        "winston": "^3.18.3"
    },
    "devDependencies": {
        "@types/bcrypt": "^6.0.0",
        "@types/bcryptjs": "^2.4.6",
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.5",
        "@types/jest": "^29.5.12",
        "@types/jsonwebtoken": "^9.0.10",
        "@types/mongodb": "^4.0.7",
        "@types/node": "^24.10.1",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^7.3.1",
        "@typescript-eslint/parser": "^7.3.1",
        "eslint": "^8.56.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.1.3",
        "jest": "^29.7.0",
        "nodemon": "^3.1.11",
        "prettier": "^3.7.3",
        "supertest": "^6.3.3",
        "ts-jest": "^29.1.2",
        "ts-node": "^10.9.2",
        "tsx": "^4.21.0",
        "typescript": "^5.3.3"
    }
}
